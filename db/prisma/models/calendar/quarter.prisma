model Quarter {
  id        Int      @id @default(autoincrement())
  idYear    Int      @map("id_year")
  year      Year     @relation(fields: [idYear], references: [id], onDelete: Cascade)
  quarter   Int
  dateBegin DateTime
  dateEnd   DateTime
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  // Relations
  months Month[]

  @@unique([idYear, quarter])
}
