model Week {
    id           Int      @id @default(autoincrement())
    id_year      Int      @map("id_year")
    year         Year     @relation(fields: [id_year], references: [id], onDelete: Cascade)
    numberInYear Int
    name         String
    dateBegin    DateTime
    dateEnd      DateTime
    createdAt    DateTime @default(now())
    updatedAt    DateTime @updatedAt

    // Relations
    days Date[]

    @@unique([id_year, numberInYear])
}
